{
    "name": "phpmentors/validator-bundle",
    "type": "symfony-bundle",
    "description": "Validation enhancements for Symfony applications",
    "keywords": ["symfony", "validator", "validation"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "GOTO Hidenori",
            "email": "hidenorigoto@gmail.com"
        },
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/annotations": "~1.0",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/validator": "~2.3"
    },
    "require-dev": {
        "phpmentors/domain-kata": "~1.3",
        "phpmentors/pageflower-bundle": "1.1.*",
        "phpunit/phpunit": "~4.0",
        "piece/stagehand-testrunner": "~4.1",
        "symfony/browser-kit": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/form": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/http-foundation": "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\ValidatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPMentors\\ValidatorBundle\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
